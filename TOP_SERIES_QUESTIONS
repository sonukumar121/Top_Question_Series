}










                                




//Rearrange Array Alternately

#include<stdio.h>
int main()
{
   //1,2,3,4,5,6.
   int a[30],i,x;
   printf("enter the elements= ");
   for(i=0;i<=5;i++)
   {
       scanf("%d",&a[i]);
   }
   
   for(x=0;x<=5;x++)
   {
       for(i=5-x;i>0;i--)
       {
           if(a[i]>a[x])
           {
           printf("%d,%d,",a[i],a[x]);
           break; 
           }
           
       }
   }
}














/*Input : 
// ARRAY SUM DIVISIBALITY ORDER.
nums = [9, 5, 7, 3]
k = 6
Output: 
True
Explanation: 
{(9, 3), (5, 7)} is a 
possible solution. 9 + 3 = 12 is divisible
by 6 and 7 + 5 = 12 is also divisible by 6.*/

#include<stdio.h>
int main()
{
    int a[30],i,j,x,n,sum,key;
    printf("enter the elments= ");
    for(i=0;i<4;i++)
    {
        scanf("%d",&a[i]);//9,5,7,3.
    }
    
    printf("enter the value of key= ");
    scanf("%d",&key); //key=6.
    
    for(x=0;x<3;x++)
    {
        for(i=x+1;i<4;i++)
        {
           sum=a[x]+a[i]; 
           if(sum%key==0)
           {
               printf("{(%d,%d)} ",a[x],a[i]);
           }
        }
    }
    
}
















//SECOND LARGEST ELEMENTS WITHOUT SORTING.

#include<stdio.h>
int main()
{
    int a[20],i,j,k,x,n,max,t,smax;
    printf("enter the elements= ");
    for(i=0;i<=4;i++)
    {
        scanf("%d",&a[i]); // 8,9,4,0,1;
    }
    max=a[0];
    for(i=0;i<=4;i++)
    {
        if(max<a[i])
        {
            max=a[i]; //max=9
            
        }
        
    }
    printf("the largest 1st elements is= %d\n",max);
    t=max;
  
    smax=a[0];
   for(i=0;i<=4;i++)
   
{   
    if(a[i]!=t)
    {
        if(smax<a[i])
        {
           smax=a[i]; 
        }
        
    }
    
}
printf("the second largest elements is= %d",smax);
}












//ZEROES TO END.


#include<stdio.h>
//Find missing number in an array.
int main()
{
    int a[20],i,j,k,x,n,c;
    printf("enter the no of value which you enter= ");
    scanf("%d",&n);
    printf("enter the elements= ");
    
    for(i=1;i<=n-1;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(i=1;i<=n-1;i++)
    {
        if(a[i+1]-a[i]==1)
        {
            continue;
        }
        else
        c=(a[i]+1);
        break;
        
        
    }
    printf("missing number is= %d",c);
    
    
}















#include<stdio.h>
int main()
{
    int a[20],i,j,k,x,n,b[30],c;
    printf("enter the elements of 1st array= ");
    for(i=0;i<=4;i++)
    {
        scanf("%d",&a[i]);//5,6,0,2,7.
    }
    
       
    printf("enter the elements  of 2nd array= ");
      for(i=0;i<=4;i++)
    {
        scanf("%d",&b[i]);//1,2,5,3,4.
    }
    
    printf("the sort 1st array= ");
    for(i=0;i<=4;i++)
    {
         for(j=i+1;j<=4;j++)
         {
             if(a[i]>a[j])
             {
                 c=a[j];
                 a[j]=a[i];
                 a[i]=c;
             }
         }
    }
    for(i=0;i<=4;i++)
    {
        printf("%d",a[i]);
    }

    printf("the sort 2nd array= ");
  
  for(i=0;i<=4;i++)
    {
         for(j=i+1;j<=4;j++)
         {
             if(b[i]>b[j])
             {
                 c=b[j];
                 b[j]=b[i];
                 b[i]=c;
             }
         }
    }
     for(i=0;i<=4;i++)
    {
        printf("%d",b[i]);
    }
    
    
  printf("the union of the both of the array elements is= ");
    for(x=0;x<=4;x++)
    {
        for(i=0;i<=4;i++)
        {
            if(a[x]>a[i])
            {
                c=a[i];
                a[i]=a[x];
                a[x]=c;
            }
        }
    }
        for(i=0;i<=4;i++)
        {

    printf("%d\n",a[i]);
        }
        for(i=0;i<=4;i++)
        {

    printf("%d",b[i]);
        }
    
    
}
