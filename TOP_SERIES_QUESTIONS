// MISSING AND REPEATED ELEMENTS IN ARRAY.
// Online C compiler to run C program online
#include <stdio.h>

int main()
{
   int a[20],b[30],i,j,n,x,m;
   printf("enter the size of 1st array= ");
   scanf("%d",&n);
   printf("enter the elements= ");
   for(i=1;i<=n;i++)
   {
       scanf("%d",&a[i]);
   }
   
   printf("enter the elements of 2nd array= ");
   scanf("%d",&m);
     printf("enter the elements= ");
     for(i=1;i<=m;i++)
     {
         scanf("%d",&b[i]);
     }
     
     for(x=1;x<=m;x++) //m=3
     {
         for(i=1;i<=n;i++) //n=5
         {
            if(b[x]==a[i]) //b[3]==a[5]
            {
                break;
            }
            if(i==n && b[x]!=a[i])
            {
                printf("No 'b' is not a subset of 'a'\n");
                break;
            }
            else
            i++;
         }
         if(x==3)
         {
          printf("yes b is a subset of a\n");   
          break;
         }
     }

   
   
          
          
     NOT DONE
}














  #include<stdio.h>
int main()
{
    int a[30],i,j,x,n,sum1=0,sum2=0,p,e,d,y;
    printf("enter the elements= ");
    for(i=1;i<=7;i++)
    {
        scanf("%d",&a[i]);
    }
    
        for(y=2;y<=7;y++)
        {
          
            for(x=2;x<=y;x++)
           {
            
            for(p=1;p<x;p++)
            {
                sum1=sum1+a[p];
            }
            d=sum1;
         }
       
        for(i=y+1;i<=7;i++)
        {
            sum2=sum2+a[i];
        }
        e=sum2;
        

        if(d==e)
        {
            printf("equilibrium point is = %d",a[x]);
        }
        else
        y++;
    }
}                             NOT DONE

 
 


//find kth smallest elements.
#include <stdio.h>

int main()
{
   int a[30],i,j,k,x,c;
   
   printf("enter the elements= ");
   for(i=1;i<=6;i++)
   {
       scanf("%d",&a[i]); //7,10,4,3,20,15.
   }
   for(i=1;i<=5;i++)
   {
       if(a[i]>a[i+1])
       {
           c=a[i];
           a[i]=a[i+1];
           a[i+1]=c;
       }
       else
       i++;
   }
   for(i=1;i<=6;i++)
   printf(" %d ",a[i]);
}                      









// k=2,n=4;
arr[]={1,5,8,10};
output=5;
the array can be modified as.
{1+k,5-k,8-k,10-k}={3,3,6,8};

the difference between largest and smallest number =8-3=5.


#include<stdio.h>
int main()
{
    int a[10],i,j,k,n;
    printf("enter the  value of k= ");
    scanf("%d",&k);
    
    printf("enter the value of n= ");
    scanf("%d",&n);
    
    printf("enter the elements= ");
    for(i=1;i<=4;i++)
    {
      scanf("%d",&a[i]);
    }
    k=a[k];
    
    for(i=1;i<=n;i++)
    {
      a[i]=a[i]-k;
    }
    for(i=1;i<=n;i++)
    {
      printf("%d",a[i]);
    }
    
}
   






