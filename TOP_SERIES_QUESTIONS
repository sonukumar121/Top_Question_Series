 // MISSING  NUMBERS IN ARRAYS.
#include<stdio.h>
int main()
//N = 10
//A[] = {6,1,2,8,3,4,7,10,5}
{
    int i,x,a[30],n;
    printf("enter the size of no.of elements till=\n ");
    scanf("%d",&n);
    printf("enter the array elements= ");
    for(i=1;i<n;i++)
    {
        scanf("%d",&a[i]);  
    }
   
    
    for(x=1;x<n;x++)
    {
    for(i=1;i<n;i++)
    {
        if(x==a[i])
        {
            break;
        }
        else if(i==9)
        {
            printf("the missing elements is= %d",x);
        }
        else
        continue;
    }






//REPEATING ELEMENTS.
#include<stdio.h>

int main() 
{
  int x,i,a[39];
  printf("enter the array elements = ");
  for(i=1;i<=4;i++)
  {
     scanf("%d",&a[i]);
  }
  for(x=1;x<=3;x++)
  {
    for(i=x;i<=3;i++)  
    {
      if(a[x]==a[i+1])
      {
          printf("the repeating  elements is =%d\n",a[x]);
          break;
          
      }
      else
      i++;
    }
  }
}






// highest elemnts in given array.
#include <stdio.h>

int main() 
{
  int i,x,max,c,a[40];
  printf("enter the array elemnsts= ");
  for(i=1;i<=5;i++)
  {
     scanf("%d",&a[i]);  
  }
   max=a[1];
   for(i=1;i<=5;i++)
   {
       if(max<a[i])
       {
           max=a[i];
           c=max;
       }
       
       else
       i++;
       
   }
   printf("the biggest elements is= %d",c);
  
}  






// HIGHEST 1ST AND 2ND ELEMENTS IN ARRAYS.
#include <stdio.h>

int main() 
{
  int i,x,max,c,a[40],h,f,e;
  printf("enter the array elemnsts= ");
  for(i=1;i<=5;i++)
  {
     scanf("%d",&a[i]);  
  }
   max=a[1];
   for(i=1;i<=5;i++)
   {
       if(max<a[i])
       {
           max=a[i];
           c=max;
       }
       
       else
       i++;
       
   }
   printf("the highest first  elements is= [%d]\n",c);
   
   
   h=c; //h=5
   f=a[1]; //f=1
   for(i=1;i<=5;i++)
   {
  
    if(f<a[i+1]) //
   {
        //f=100
       if(f==h) //5==5
       {
         continue;  
       }
       f=a[i+1];//f=2
       e=f; // 2
       f=e;//2
   }
   
   else
   i++;
   printf("the second highest elemnts is= [%d]\n",f);
   break;
  
}  
}






