    scanf("%d",&n);
    printf("enter the array elements= ");
    for(i=1;i<n;i++)
    {
        scanf("%d",&a[i]);  
    }
   
    
    for(x=1;x<n;x++)
    {
    for(i=1;i<n;i++)
    {
        if(x==a[i])
        {
            break;
        }
        else if(i==9)
        {
            printf("the missing elements is= %d",x);
        }
        else
        continue;
    }


// repating elements .
#include<stdio.h>

int main() 
{
  int x,i,a[39];
  printf("enter the array elements = ");
  for(i=1;i<=4;i++)
  {
     scanf("%d",&a[i]);
  }
  for(x=0;x<=2;x++)
  {
    for(i=x;i<=2;i++)  
    {
      if(a[x]==a[i+1])
      {
          printf("the repeating  elements is =%d",a[x]);
          
      }
      else
      i++;
    }
  }
}
  
    }
    
}
