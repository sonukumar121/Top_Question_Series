#include<stdio.h>
//Intersection of two arrays

int main()
{
   int a[20],b[30],i,j,n;
   printf("enter the array elements of a= ");
   for(i=1;i<=5;i++)
   {
       scanf("%d",&a[i]);//1,2,3,4,5.
   }
   
   printf("enetr the array elements of b=  ");
   for(i=1;i<=3;i++)
   {
       scanf("%d",&b[i]); //1,2,4.
   }
   
   for(j=1;j<=3;j++)
   {
       for(i=j;i<=j;i++)
       {
           if(a[i]=a[j])
           {
            printf("%d",a[i]);
               break;
           }
       }

   }
   
}   // NOT DONE.








// FIND ELEMENTS WHICH OCCUR 'K' TIMES.
#include<stdio.h>
int main()
{
    int a[33],i,j,x,c=0;
    printf("enter the array elements= ");
    for(i=1;i<=7;i++)
    {
        scanf("%d",&a[i]);
    }

   for(x=1;x<=5;x++)
   {
     for(i=x+1;i<=7;i++)
     {
         if(a[x]==a[i])
         {
             c=c+1;
            break;
         }
         
         else
         i++;
     }
   }
   printf("the no.of elements repeated is = %d",c);
}





//ALL ZEROES AT THE END OF THE ARRAY ELEMENTS OR SET OF NUMBERS.
#include<stdio.h>
int main()
//Move all zeroes to end of the array elements.
{
  int a[20],i,n,x,c;
  printf("enter the value of n= ");
  scanf("%d",&n);
  
  printf("enetr the array elements= ");
  for(i=1;i<=n;i++)
  {
     scanf("%d",&a[i]); //1,2,0,0,3,4 ==>1,2,4,0,3,0
  }
  
for(x=0;x<=5;x++)
{
  for(i=1;i<=n;i++)
  {
    if(a[i]==0) // a[3]==0          a[4]==0
  {
     c=a[i]; //c=0
     a[i]=a[n-x];// a[3]=a[6]==>a[3]=3
     a[n]=c;// a[6]=0
     break;
     
    
  }
  else
  i++;
}

}
for(i=1;i<=6;i++)
{
    printf(" %d",a[i]);
}
} ====> NOT DONE.
   








